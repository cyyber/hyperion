// Sending zero zond to a contract should still invoke the receive zond function
// (it previously did not because the gas stipend was not provided by the ZVM)
contract Receiver {
    receive() external payable {}
}


contract Main {
    constructor() payable {}

    function s() public returns (bool) {
        Receiver r = new Receiver();
        return payable(r).send(0);
    }
}
