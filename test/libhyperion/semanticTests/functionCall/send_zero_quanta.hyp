// Sending zero quanta to a contract should still invoke the receive quanta function
// (it previously did not because the gas stipend was not provided by the QRVM)
contract Receiver {
    receive() external payable {}
}


contract Main {
    constructor() payable {}

    function s() public returns (bool) {
        Receiver r = new Receiver();
        return payable(r).send(0);
    }
}
