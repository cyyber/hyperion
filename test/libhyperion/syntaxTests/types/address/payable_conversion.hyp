contract C {
    function f() public {
        address payable a = payable(address(new D()));
        address payable b = payable(new E());
        address payable c = payable(new F());

        a;
        b;
        c;
    }
}

// A contract that cannot receive ZND
contract D {}

// A contract that can receive ZND
contract E {
    receive() external payable {
    }
}

// A contract that can receive ZND using the fallback
contract F {
    fallback() external payable {

    }
}

